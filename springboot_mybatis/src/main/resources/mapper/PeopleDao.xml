<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.springboot_mybatis.dao.PeopleDao">
    <select id="findById" resultType="com.example.springboot_mybatis.pojo.People" parameterType="int">
        select * from people where id=#{id}
    </select>
</mapper>